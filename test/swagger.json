{
  "swagger": "2.0",
  "info": {
    "title": "Swagger clientgen",
    "version": "1.0.0"
  },
  "paths": {
    "/something": {
      "get": {
        "operationId": "getAllThings",
        "responses": {
          "200": {
            "description": "A list of all available things",
            "schema": {
              "type": "array",
              "items": {
                "properties": {
                  "id": {
                    "type": "integer"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "operationId": "createThing",
        "parameters": [
          {
            "name": "thing",
            "in": "body"
          }
        ]
      }
    },
    "/something/{id}": {
      "delete": {
        "operationId": "deleteThing",
        "parameters": [
          {
            "name": "id",
            "in": "path"
          }
        ]
      },
      "put": {
        "operationId": "updateThing",
        "parameters": [
          {
            "name": "id",
            "in": "path"
          },
          {
            "name": "thing",
            "in": "body"
          }
        ]
      }
    },
    "/query": {
      "get": {
        "operationId": "searchQuery",
        "parameters": [
          {
            "name": "q",
            "in": "query"
          }
        ]
      }
    },
    "/header": {
      "head": {
        "operationId": "sendHeader",
        "parameters": [
          {
            "name": "token",
            "in": "header"
          }
        ]
      },
      "options": {
        "operationId": "optionsHeader"
      },
      "patch": {
        "operationId": "patchHeader"
      }
    }
  }
}
